     &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_index_on_measurements_table&quot;&gt;&#10;        &lt;sql&gt;&#10;&#9;CREATE INDEX measurements_idx&#10;  &#9;ON soop_xbt_dm.measurements&#10;  &#9;USING btree&#10; &#9; (profile_id);&#10;        &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_qc_flags_table&quot;&gt;&#10;        &lt;sql&gt;&#10;        CREATE TABLE qc_flags&#10;        (&#10;          profile_id bigint NOT NULL,&#10;          &quot;TIME_quality_control&quot; smallint,&#10;          &quot;LONGITUDE_quality_control&quot; smallint,&#10;          &quot;LATITUDE_quality_control&quot; smallint,&#10;          CONSTRAINT qc_flags_pk PRIMARY KEY (profile_id),&#10;          CONSTRAINT qc_flags_fkey FOREIGN KEY (profile_id)&#10;              REFERENCES indexed_file (id) MATCH SIMPLE&#10;              ON UPDATE CASCADE ON DELETE CASCADE&#10;        );&#10;        &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_xbt_lines_table&quot;&gt;&#10;        &lt;sql&gt;&#10;        CREATE TABLE xbt_lines&#10;        (&#10;          &quot;XBT_line&quot; character varying(20) NOT NULL,&#10;          colour character varying(7),&#10;          CONSTRAINT xbt_lines_pkey PRIMARY KEY (&quot;XBT_line&quot;)&#10;        );&#10;        &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_ soop_xbt_dm_profile_map_table&quot;&gt;&#10;        &lt;sql&gt;&#10;        CREATE TABLE soop_xbt_dm_profile_map&#10;        (&#10;          profile_id bigint NOT NULL,&#10;          url character varying(512) NOT NULL,&#10;          &quot;XBT_line&quot; character varying(20) NOT NULL,&#10;          &quot;XBT_line_description&quot; character varying(50),&#10;          &quot;Callsign&quot; character varying(10),&#10;          &quot;XBT_cruise_ID&quot; character varying(20),&#10;          &quot;XBT_uniqueid&quot; character varying(12),&#10;          &quot;TIME&quot; timestamp with time zone,&#10;          max_depth double precision,&#10;         &quot;XBT_probetype_fallrate_equation&quot; character varying(256),&#10;          &quot;XBT_recorder_type&quot; character varying(256),&#10;          &quot;XBT_instrument_serialnumber&quot; character varying(10),&#10;          &quot;XBT_calibration_OFFS&quot; character varying(10),&#10;          &quot;XBT_calibration_SCAL&quot; character varying(10),&#10;          colour character varying(7),&#10;          geom geometry(Geometry,4326),&#10;&#9;  size double precision,&#10;          CONSTRAINT soop_xbt_dm_profile_map_pkey PRIMARY KEY (profile_id),&#10;          CONSTRAINT soop_xbt_dm_profile_map_fkey FOREIGN KEY (profile_id)&#10;              REFERENCES indexed_file (id) MATCH SIMPLE&#10;              ON UPDATE CASCADE ON DELETE CASCADE&#10;        );&#10;        &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;    &#10;    &#9;&lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_gist_index_and_add_geom_check_on_map_table&quot;&gt;&#10;&#9;      &lt;sql&gt;&#10;&#9;        CREATE INDEX soop_xbt_dm_profile_map_gist_idx ON soop_xbt_dm_profile_map USING GIST (geom);&#10;&#9;&#9;ALTER TABLE soop_xbt_dm_profile_map ADD CHECK (ST_ISVALID(geom));&#10;&#9;      &lt;/sql&gt;&#10;&#9;    &lt;/changeSet&gt;&#10;&#10;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_ soop_xbt_dm_profile_data_view&quot; runOnChange=&quot;true&quot;&gt;&#10;&#9;&lt;sql&gt;&#10;       &#9;DROP VIEW IF EXISTS soop_xbt_dm_profile_data;&#10;       &#9;CREATE VIEW soop_xbt_dm_profile_data AS&#10;&#9;SELECT measurements.profile_id,&#10;         measurements.measurement_id, &#10;         soop_xbt_dm_profile_map.&quot;XBT_line&quot;,&#10;         soop_xbt_dm_profile_map.&quot;XBT_line_description&quot;,&#10;         soop_xbt_dm_profile_map.&quot;Callsign&quot;,&#10;         soop_xbt_dm_profile_map.&quot;XBT_cruise_ID&quot;,&#10;         soop_xbt_dm_profile_map.&quot;XBT_uniqueid&quot;,&#10;         soop_xbt_dm_profile_map.&quot;XBT_probetype_fallrate_equation&quot;,&#10;         soop_xbt_dm_profile_map.&quot;XBT_recorder_type&quot;,&#10;         soop_xbt_dm_profile_map.&quot;XBT_instrument_serialnumber&quot;,&#10;         soop_xbt_dm_profile_map.&quot;XBT_calibration_OFFS&quot;,&#10;         soop_xbt_dm_profile_map.&quot;XBT_calibration_SCAL&quot;,&#10;         soop_xbt_dm_profile_map.&quot;TIME&quot; AT TIME ZONE 'UTC' AS &quot;TIME&quot;,&#10;         qc_flags.&quot;TIME_quality_control&quot;, &#10;         st_x(soop_xbt_dm_profile_map.geom) AS &quot;LONGITUDE&quot;, &#10;         qc_flags.&quot;LONGITUDE_quality_control&quot;, &#10;         st_y(soop_xbt_dm_profile_map.geom) AS &quot;LATITUDE&quot;, &#10;         qc_flags.&quot;LATITUDE_quality_control&quot;, &#10;         measurements.&quot;DEPTH&quot;, &#10;         measurements.&quot;DEPTH_quality_control&quot;, &#10;         measurements.&quot;TEMP&quot;, &#10;         measurements.&quot;TEMP_quality_control&quot;, &#10;         soop_xbt_dm_profile_map.geom&#10;         FROM measurements&#10;         LEFT JOIN soop_xbt_dm_profile_map ON soop_xbt_dm_profile_map.profile_id = measurements.profile_id&#10;         LEFT JOIN qc_flags ON qc_flags.profile_id = measurements.profile_id&#10;         ORDER BY measurements.measurement_id;&#10;&#9;&lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;alter_soop_xbt_dm_profile_map_table&quot;&gt;&#10;        &lt;sql&gt;&#10;        ALTER TABLE soop_xbt_dm_profile_map ADD COLUMN nb_measurements integer;&#10;        &lt;/sql&gt;&#10;    &lt;/changeSet&gt;&#10;&#10;&lt;/databaseChangeLog&gt; "/>
