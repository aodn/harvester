    <elementParameter field="CHECK" name="USE_INLINE_CONTENT" value="true"/>
    <elementParameter field="MEMO" name="INLINE_CONTENT" value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?>&#xA;&lt;databaseChangeLog &#xA;    xmlns=&quot;http://www.liquibase.org/xml/ns/dbchangelog&quot;&#xA;    xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&#xA;    xsi:schemaLocation=&quot;http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd&quot;>&#xA;&#xA;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_attribute_table&quot;>&#xA;        &lt;sql>&#xA;  CREATE TABLE attribute&#xA;  (&#xA;     file_id bigint,&#xA;     url text,&#xA;     callsign text,&#xA;     vessel_name text,&#xA;     &quot;timestamp&quot; timestamp(6) with time zone,&#xA;     longitude double precision,&#xA;     latitude double precision,&#xA;     geom geometry(Geometry,4326),&#xA;    CONSTRAINT attribute_pkey PRIMARY KEY (file_id),&#xA;      CONSTRAINT attribute_fkey FOREIGN KEY (file_id)&#xA;          REFERENCES indexed_file (id) MATCH SIMPLE&#xA;          ON UPDATE CASCADE ON DELETE CASCADE&#xA;  );&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_measurements_table&quot;>&#xA;        &lt;sql>&#xA;  CREATE TABLE measurements&#xA;  (&#xA;     measurement_id bigserial NOT NULL,&#xA;      profile_id integer,&#xA;     depth real,&#xA;     temperature real,&#xA;    CONSTRAINT measurements_pkey PRIMARY KEY (measurement_id),&#xA;      CONSTRAINT measurements_fkey FOREIGN KEY (profile_id)&#xA;          REFERENCES indexed_file (id) MATCH SIMPLE&#xA;          ON UPDATE CASCADE ON DELETE CASCADE&#xA;  );&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;    &lt;changeSet author=&quot;lbesnard&quot; id=&quot;create_soop_xbt_lines_table&quot;>&#xA;        &lt;sql>&#xA;    CREATE TABLE xbt_lines&#xA;    (&#xA;      callsign character varying(20) NOT NULL,&#xA;      colour character varying(7),&#xA;      CONSTRAINT callsign_pkey PRIMARY KEY (callsign)&#xA;    )&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_soop_xbt_nrt_profiles_map_table&quot;>&#xA;        &lt;sql>&#xA;      CREATE TABLE soop_xbt_nrt_profiles_map&#xA;      (&#xA;      profile_id bigint NOT NULL,&#xA;      url text,&#xA;        &quot;Callsign&quot; character varying(10) NOT NULL,&#xA;        vessel_name character varying(100),&#xA;        &quot;TIME&quot; timestamp(6) with time zone,&#xA;        nb_measurements integer,&#xA;        max_depth integer,&#xA;        colour character varying(7),&#xA;      geom geometry(Geometry,4326),&#xA;      size double precision,&#xA;        CONSTRAINT soop_xbt_nrt_profiles_map_pkey PRIMARY KEY (profile_id)&#xA;      )&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;add_index_and_srid_to_map&quot;>&#xA;        &lt;sql>&#xA;      CREATE INDEX attribute_gist_idx ON attribute USING GIST (geom); &#xA;      CREATE INDEX soop_xbt_nrt_profiles_map_gist_idx ON soop_xbt_nrt_profiles_map USING GIST (geom); &#xA;      ALTER TABLE attribute ADD CHECK (ST_IsValid(geom));&#xA;      ALTER TABLE soop_xbt_nrt_profiles_map ADD CHECK (ST_IsValid(geom));&#xA;  &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;&#xA;    &lt;changeSet author=&quot;xhoenner&quot; id=&quot;create_soop_xbt_nrt_profiles_data_view&quot; runOnChange=&quot;true&quot;>&#xA;        &lt;sql> &#xA;    DROP VIEW IF EXISTS soop_xbt_nrt_profiles_data;&#xA;    CREATE OR REPLACE VIEW soop_xbt_nrt_profiles_data AS &#xA;    SELECT m.profile_id, &#xA;        m.measurement_id, &#xA;        ma.&quot;Callsign&quot;, &#xA;        ma.vessel_name, &#xA;        a.timestamp AT TIME ZONE 'UTC' AS &quot;TIME&quot;, &#xA;        a.longitude AS &quot;LONGITUDE&quot;, &#xA;        a.latitude &quot;LATITUDE&quot;, &#xA;        m.depth AS &quot;DEPTH&quot;, &#xA;        m.temperature AS &quot;TEMP&quot;,&#xA;        ma.geom&#xA;        FROM measurements m&#xA;        LEFT JOIN soop_xbt_nrt_profiles_map ma ON ma.profile_id = m.profile_id&#xA;      LEFT JOIN attribute a ON a.file_id = m.profile_id&#xA;        ORDER BY m.profile_id, m.measurement_id;&#xA;        &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;&lt;/databaseChangeLog> "/>
