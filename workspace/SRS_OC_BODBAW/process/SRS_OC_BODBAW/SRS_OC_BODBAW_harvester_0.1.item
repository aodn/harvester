      &lt;/sql>&#xA;    &lt;/changeSet>&#xA;&#xA;    &#xA;&#xA; &lt;changeSet author=&quot;talend&quot; id=&quot;1382580250154-3&quot;>&#xA;       &lt;sql>&#xA;ALTER TABLE measurements&#xA;ALTER COLUMN geom TYPE geometry(GEOMETRY, 4326)&#xA;USING ST_SetSRID(geom,4326);&#xA;CREATE INDEX measurements_gist_idx ON measurements USING GIST (geom); &#xA; &lt;/sql>&#xA;       &#xA;    &lt;/changeSet>&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;cruise_id_colour&quot;>&#xA;       &lt;sql>&#xA;CREATE TABLE cruise_id&#xA;(&#xA;  cruise_id TEXT NOT NULL,&#xA;  vessel_name TEXT NOT NULL,&#xA;  platform_code TEXT,&#xA;  colour TEXT,&#xA;  CONSTRAINT cruise_id_pkey PRIMARY KEY (cruise_id )&#xA;)&#xA;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&#xA;&#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;1382580250154-5&quot;>&#xA;       &lt;sql>&#xA;CREATE TABLE srs_oc_bodbaw_trajectory_profile_map&#xA;(&#xA;  file_id integer,&#xA;  abstract text,&#xA;  url_marlin_metadata text,&#xA;  cruise_id TEXT,&#xA;  data_type text,&#xA;  url_nc text,&#xA;  url_csv text,&#xA;  url_plot text,&#xA;  size double precision,&#xA;  time_start timestamp without time zone,&#xA;  time_end timestamp without time zone,&#xA;  vessel_name TEXT,&#xA;  colour TEXT,&#xA;  geom geometry,&#xA;  CONSTRAINT measurements_geom_check CHECK (st_isvalid(geom)),&#xA;  CONSTRAINT file_id PRIMARY KEY (file_id),&#xA;  CONSTRAINT fk_file_id FOREIGN KEY (file_id)&#xA;      REFERENCES indexed_file (id) MATCH SIMPLE&#xA;      ON UPDATE CASCADE ON DELETE CASCADE&#xA;)&#xA;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;1382580250154-6&quot;>&#xA;        &lt;sql>&#xA;ALTER TABLE srs_oc_bodbaw_trajectory_profile_map&#xA;ALTER COLUMN geom TYPE geometry(GEOMETRY, 4326)&#xA;USING ST_SetSRID(geom,4326);&#xA;CREATE INDEX srs_oc_bodbaw_trajectory_profile_map_gist_idx ON srs_oc_bodbaw_trajectory_profile_map USING GIST (geom); &#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&#xA;&lt;changeSet author=&quot;talend&quot; id=&quot;rename_map_col&quot;>&#xA;        &lt;sql>&#xA;        ALTER TABLE srs_oc_bodbaw_trajectory_profile_map RENAME time_start TO time_coverage_start;&#xA;        ALTER TABLE srs_oc_bodbaw_trajectory_profile_map RENAME time_end TO time_coverage_end;&#xA;&#x9;&lt;/sql>&#xA; &lt;/changeSet>&#xA;&#xA;&lt;/databaseChangeLog>"/>
